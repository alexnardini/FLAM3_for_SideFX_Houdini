                    pane panetab6 -a 0 -l 10 -H /obj/geo1/ -T SOP -m parmeditor
                }
            }
        }
        tabs 0 1 {
            pane panetab7 -a 0 -l 10 -H /obj/geo1/ -T SOP -m neteditor
        }
    }
}
