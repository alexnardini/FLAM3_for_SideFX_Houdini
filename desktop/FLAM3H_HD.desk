fullscreen   -1
playbarshown 1
playbar      -1
shelf        -1
menubar      -1
statusbar    -1
radial menu set main
root {
    split 0.626042 H {
        split 0.118462 H L {
            tabs 0 1 {
                pane panetab14 -a 0 -l 10 -H /obj/geo1/ -T SOP -m treecontrol
            }
            split 0.585774 H {
                tabs 0 1 {
                    pane panetab1 -a 0 -l 10 -H /obj/geo1/ -T SOP -m sceneview
                    pane panetab2 -a 0 -l 10 -H /obj/geo1/ -T SOP -m details
                }
